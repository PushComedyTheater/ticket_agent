  <script type="text/javascript">
  window.csrf_token = <%= raw Poison.encode!(Plug.CSRFProtection.get_csrf_token()) %>;
  window.stripe_publishable_key = "<%= TicketAgent.Services.Stripe.publishable_key %>";
  </script>
  <script src="https://js.stripe.com/v3/"></script>
  <script src="https://cdn.ravenjs.com/3.21.0/raven.min.js" crossorigin="anonymous"></script>
  <script type="text/javascript">
    Raven.config("<%= System.get_env("SENTRY_PUBLIC_DSN") %>", {
      release: "<%= Application.get_env(:ticket_agent_web, :release, "unknown") %>"
    }).install();
    <%= if Coherence.logged_in?(@conn) do %>
    Raven.setUserContext({
      id: "<%= Coherence.current_user_name(@conn, :id) %>",
      email: "<%= Coherence.current_user_name(@conn, :email) %>",
      username: "<%= Coherence.current_user_name(@conn) %>",
    });
    <% end %>
  </script>
