      <!-- Breadcrumbs -->
      <%= render "breadcrumbs.html", assigns %>
      <!-- End Breadcrumbs -->
            <!-- Figure -->
      <section class="container g-pt-100 g-pb-40">
        <figure class="row">
          <!-- Figure Image -->
          <div class="col-md-8 offset-md-2 col-lg-4 offset-lg-0 g-mb-30">
            <img class="w-100" src="https://cdn.pushcomedytheater.com/images/<%= @pusher.slug %>.jpg" alt="Image Description">
          </div>
          <!-- End Figure Image -->

          <!-- Figure Body -->
          <div class="col-lg-8">
            <div class="d-flex justify-content-between g-mb-10">
              <div class="g-mb-20">
                <h4 class="h5 g-mb-5"><%= @pusher.name %></h4>
                <em class="d-block g-font-style-normal g-font-size-default text-uppercase g-color-primary">Instructor</em>
              </div>
              <!-- Figure Social Icons -->
              <ul class="list-inline">
                  <li class="list-inline-item g-mx-2">
                    <a class="u-icon-v1 u-icon-size--sm u-icon-slide-up--hover g-color-gray-light-v1 g-bg-gray-light-v5 g-color-gray-light-v1--hover rounded-circle" href="mailto:<%= @pusher.email %>">
                      <i class="g-font-size-default g-line-height-1 u-icon__elem-regular fa fa-envelope"></i>
                      <i class="g-font-size-default g-line-height-0_8 u-icon__elem-hover fa fa-envelope"></i>
                    </a>
                  </li>
                <%= for {name, link} <- @pusher.social do %>
                  <li class="list-inline-item g-mx-2">
                    <a class="u-icon-v1 u-icon-size--sm u-icon-slide-up--hover g-color-gray-light-v1 g-bg-gray-light-v5 g-color-gray-light-v1--hover rounded-circle" href="<%= link %>">
                      <i class="g-font-size-default g-line-height-1 u-icon__elem-regular fa fa-<%= name %>"></i>
                      <i class="g-font-size-default g-line-height-0_8 u-icon__elem-hover fa fa-<%= name %>"></i>
                    </a>
                  </li>
                <% end %>
              </ul>
              <!-- End Figure Social Icons -->
            </div>

            <!-- Figure Info -->
            <div class="g-mb-50">
              <%= @pusher.biography |> raw() %>
            </div>
            <!-- End Info -->
          </div>
          <!-- End Figure Body -->
        </figure>
      </section>
      <%= render "team.html", assigns %>
