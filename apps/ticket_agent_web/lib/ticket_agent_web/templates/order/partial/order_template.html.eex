<script id="entry-template" type="text/x-handlebars-template">
  <tr>
    <td class="g-color-black-opacity-0_8" scope="row">{{ticket_name}} (Ticket For {{name}})</td>
    <td>{{price}}</td>
  </tr>
</script>
<script id="calculated-template" type="text/x-handlebars-template">
  <tr>
    <td class="g-color-black-opacity-0_8" align="right" style="text-align: right;" scope="row">{{title}}: </td>
    <td>{{value}}</td>
  </tr>
</script>
