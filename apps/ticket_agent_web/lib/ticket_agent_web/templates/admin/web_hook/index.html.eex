 <section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Webhooks</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body table-responsive no-padding">
          <p style="padding: 10px;">Webhooks</p>

            <table class="table table-hover">
              <thead>
                <tr>
                  <th></th>
                  <th>Source</th>
                  <th>Date</th>
                </tr>
              </thead>
              <tbody>
            <%= for detail <- @details do %>
                <tr>
                  <td>
                    <span><%= link "Show", to: admin_webhook_path(@conn, :show, detail), class: "btn btn-info btn-xs" %></span>
                  </td>
                  <td><%= detail.source %></td>
                  <td><%= order_timestamp(detail.inserted_at) %></td>
                </tr>
            <% end %>
              </tbody>
            </table>

        </div>
<div class="box-footer clearfix">
<div class="pull-right">
  <%= pagination_links @details %>
  </div>
            </div>        
        <!-- /.box-body -->
      </div>
      <!-- /.box -->
    </div>
  </div>
</section>
          <!-- /.box -->
