 <section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Listings</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body table-responsive no-padding">
          <table class="table table-hover">
            <tbody><tr>
              <th width="250px;">Actions</th>
              <th>Title</th>
              <th>Start Time</th>
              <th>End Time</th>
            </tr>
            <%= for listing <- @listings do %>
              <tr>
                <td align="center">
                  <span><%= link "View", to: admin_event_path(@conn, :show, listing), class: "btn btn-info btn-xs" %></span>
                  <span><%= link "Edit", to: admin_event_path(@conn, :edit, listing), class: "btn btn-warning btn-xs" %></span>
                  <span><%= link "Delete", to: admin_event_path(@conn, :delete, listing), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %></span>
                </td>
                <td><%= listing.title %></td>
                <td><%= order_timestamp(listing.start_at) %></td>
                <td><%= order_timestamp(listing.end_at) %></td>
              </tr>
            <% end %>
          </tbody></table>
        </div>
<div class="box-footer clearfix">
<div class="pull-right">
  <%= pagination_links @listings %>
  </div>
            </div>        
        
        <!-- /.box-body -->
      </div>
      <!-- /.box -->
    </div>
  </div>
</section>
          <!-- /.box -->
